{
  "profiles": {
    "msg * asd": {
      "commandName": "Project",
      "commandLineArgs": "msg /v /w * ASD"
    },
    "--dont-wait msg * asd": {
      "commandName": "Project",
      "commandLineArgs": "--dont-wait msg /v /w * ASD"
    },
