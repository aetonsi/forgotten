      "commandName": "Project",
      "commandLineArgs": "--dont-wait php -r \"sleep(3);exit(rand(1,255));\""
    },
    "sleep+errorlevel": {
      "commandName": "Project",
      "commandLineArgs": "php -r \"sleep(3);exit(rand(1,255));\""
    }
  }
